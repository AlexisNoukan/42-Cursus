#include "stdio.h"
#include "string.h"

void	*ft_memset(void *s, int c, int len)
{
	char	*str;
	int		i;

	str = s;
	i = -1;
	while (++i < len)
		str[i] = (char)c;
	s = str;
	return (s);
}

int	main()
{
//	printf("%s/n", memset("Hello World", 0, 4));
	printf("%s", ft_memset("Hello World", 0, 4));
}
